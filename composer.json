{
  "name": "shiftedreality/magento-composer-installer-bridge",
  "description": "Composer installer for Magento modules",
  "type": "composer-plugin",
  "license": "OSL-3.0",
  "require": {
    "php": "^7.0",
    "composer-plugin-api": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Wikimedia\\Composer\\": "src/Composer/src",
      "MagentoHackathon\\Composer\\Magento\\": "src/Magento/src/MagentoHackathon/Composer/Magento"
    }
  },
  "replace": {
    "magento/magento-composer-installer": "*",
    "magento-hackathon/magento-composer-installer": "*",
    "wikimedia/composer-merge-plugin": "*"
  },
  "extra": {
    "composer-command-registry": [
      "MagentoHackathon\\Composer\\Magento\\Command\\DeployCommand"
    ],
    "class": [
      "Wikimedia\\Composer\\MergePlugin",
      "MagentoHackathon\\Composer\\Magento\\Plugin"
    ]
  }
}
